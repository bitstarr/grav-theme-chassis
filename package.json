    "dependencies": {},
    "devDependencies": {
        "gulp": "*",
        "gulp-imagemin": "*",
        "gulp-plumber": "*",
        "gulp-postcss": "*",
        "gulp-sourcemaps": "*",
        "gulp-newer": "*",
        "gulp-cssnano": "*",
        "gulp-concat": "*",
        "gulp-terser": "*",
        "gulp-rev": "*",
        "gulp-livereload": "*",
        "autoprefixer": "*",
        "cssnano": "*",
        "postcss": "*",
        "postcss-easy-import": "*",
        "postcss-nested": "*",
        "postcss-media-variables": "*",
        "imagemin": "*",
        "imagemin-giflossy": "*",
        "imagemin-mozjpeg": "*",
        "imagemin-pngquant": "*",
        "imagemin-zopfli": "*",
        "imagemin-svgo": "*",
        "eslint": "*",
        "stylelint": "*",
        "browser-sync": "*",
        "npm-run-all": "*",
        "glob": "*",
        "del": "*",
        "ruleemall": "https://github.com/bitstarr/ruleemall/"
    },
    "config": {
        "css": "assets/css/",
        "cssDist": "dist/css/",
        "js": "assets/js/",
        "jsSrc": "assets/js/src/",
        "jsDist": "dist/js/",
        "img": "assets/img/",
        "imgDist": "dist/img/",
        "icons": "assets/icons/",
        "iconsDist": "dist/icons/",
        "favicons": "assets/favicon/",
        "faviconsDist": "dist/",
        "fonts": "assets/fonts/",
        "fontsDist": "dist/fonts/",
        "assets": "assets/",
        "dist": "dist/",
        "bsProxy": "HOSTNAME.ddev.site"
    },
    "scripts": {
        "js": "npx gulp js",
        "jsmin": "npx gulp jsmin",
        "css": "npx gulp css",
        "cssmin": "npx gulp cssmin",
        "fonts": "npx gulp fonts",
        "lint:css": "npx stylelint $npm_package_config_css",
        "lint:js": "npx eslint $npm_package_config_js",
        "lint": "npm-run-all --parallel lint:*",
        "img": "npx gulp images",
        "img:sloppy": "npx gulp sloppyImg",
        "icons": "npx gulp icons",
        "imagemin": "npx gulp imagemin",
        "favicons": "npx gulp favicons",
        "watch": "npx gulp watch",
        "todo": "grep -lir --color --exclude-dir=node_modules --exclude-dir=vendor --exclude-dir=var --exclude=package.json 'todo'",
        "clean": "rm -rf $npm_package_config_dist/*",
        "dev": "npx gulp dev",
        "build": "npm run lint && npx gulp build"
    }
}
